	<subDataset name="New Dataset 1">
		<parameter name="C_Order_ID" class="java.lang.String"/>
		<queryString>
			<![CDATA[select distinct c_order.documentno,m_product.name as itemrangecode,m_attributevalue.name as description,
	m_attributevalue.em_dtpo_area as area,m_attributevalue.em_dtpo_make as make,m_attributevalue.em_dtpo_model as model,
	m_attributevalue.EM_Dtpo_Itemno as itemno,m_attributevalue.EM_Dtpo_Hsncode as hscode,m_attributevalue.EM_Dtpo_Countryoforigin as country,
	m_attributevalue.EM_Dtpo_Portofloading as portofloading,m_attributevalue.EM_Dtpo_Uom as uom,c_orderline.qtyordered as qty,
	c_orderline.em_dtpo_importbasic as basic, c_orderline.em_dtpo_insurance::numeric as insurance, c_orderline.em_dtpo_frieght as freight,
	c_orderline.em_dtpo_pkg as packingandforwarding,c_orderline.em_dtpo_others::numeric as other,c_orderline.em_dtpo_supplytotal as importedsupplyratetotal,
	c_orderline.linenetamt as amount
	from c_orderline
left join c_order on c_order.c_order_id=c_orderline.c_order_id
left join m_product on m_product.m_product_id=c_orderline.m_product_id
left join m_attributesetinstance on m_attributesetinstance.m_attributesetinstance_id=c_orderline.m_attributesetinstance_id
left join m_attributeset on m_attributeset.m_attributeset_id=m_attributesetinstance.m_attributeset_id
left join m_attributeuse on m_attributeuse.m_attributeset_id=m_attributeset.m_attributeset_id
left join m_attributevalue on m_attributevalue.m_attribute_id=m_attributeuse.m_attribute_id

where c_order.issotrx='N' and em_dtpo_orderscenario='TYPE1' and c_order.c_order_id=$P{C_Order_ID} order by m_product.name]]>
		</queryString>
		<field name="documentno" class="java.lang.String"/>
		<field name="itemrangecode" class="java.lang.String"/>
		<field name="description" class="java.lang.String"/>
		<field name="area" class="java.lang.String"/>
		<field name="make" class="java.lang.String"/>
		<field name="model" class="java.lang.String"/>
		<field name="itemno" class="java.lang.String"/>
		<field name="hscode" class="java.lang.String"/>
		<field name="country" class="java.lang.String"/>
		<field name="portofloading" class="java.lang.String"/>
		<field name="uom" class="java.lang.String"/>
		<field name="qty" class="java.math.BigDecimal"/>
		<field name="basic" class="java.math.BigDecimal"/>
		<field name="insurance" class="java.math.BigDecimal"/>
		<field name="freight" class="java.math.BigDecimal"/>
		<field name="packingandforwarding" class="java.math.BigDecimal"/>
		<field name="other" class="java.math.BigDecimal"/>
		<field name="importedsupplyratetotal" class="java.math.BigDecimal"/>
		<field name="amount" class="java.math.BigDecimal"/>
		<variable name="AUTO_COUNT" class="java.lang.Integer">
			<variableExpression><![CDATA[new Integer($V{AUTO_COUNT}.intValue() + 1)]]></variableExpression>
			<initialValueExpression><![CDATA[new Integer(0)]]></initialValueExpression>
		</variable>
		<variable name="net_insurance" class="java.math.BigDecimal">
			<variableExpression><![CDATA[($F{qty}.multiply( $F{insurance} )
    )]]></variableExpression>
		</variable>
		<variable name="total_insurance" class="java.math.BigDecimal" calculation="Sum">
			<variableExpression><![CDATA[$V{net_insurance}]]></variableExpression>
		</variable>
		<variable name="net_pkg" class="java.math.BigDecimal">
			<variableExpression><![CDATA[$F{qty}.multiply($F{packingandforwarding} )]]></variableExpression>
		</variable>
		<variable name="Total_pkg" class="java.math.BigDecimal" calculation="Sum">
			<variableExpression><![CDATA[$V{net_pkg}]]></variableExpression>
		</variable>
		<variable name="Net_Frieght" class="java.math.BigDecimal">
			<variableExpression><![CDATA[$F{qty}.multiply($F{freight} )]]></variableExpression>
		</variable>
		<variable name="Total_frieght" class="java.math.BigDecimal" calculation="Sum">
			<variableExpression><![CDATA[$V{Net_Frieght}]]></variableExpression>
		</variable>
		<variable name="Net_Other" class="java.math.BigDecimal">
			<variableExpression><![CDATA[$F{qty}.multiply( $F{other})]]></variableExpression>
		</variable>
		<variable name="total_other" class="java.math.BigDecimal" calculation="Sum">
			<variableExpression><![CDATA[$V{Net_Other}]]></variableExpression>
		</variable>
		<variable name="Grand_Total" class="java.math.BigDecimal">
			<variableExpression><![CDATA[$V{total_insurance}.add($V{Total_pkg} ).add( $V{Total_frieght} ).add( $V{total_other} )]]></variableExpression>
		</variable>
	</subDataset>
	<parameter name="C_Order_ID" class="java.lang.String"/>
	<queryString>
		<![CDATA[select ad_org.ad_org_id,ad_org.name as orgname,documentno,docstatus,dateordered,c_bpartner.name as bpartner,c_bpartner_location.name as bpartneraddress,c_paymentterm.name as paymentterm,c_project.name as projectname,em_dtpo_paymentdesc from c_order
left join ad_org on ad_org.ad_org_id=c_order.ad_org_id
left join c_bpartner on c_bpartner.c_bpartner_id=c_order.c_bpartner_id
left join c_bpartner_location on c_bpartner_location.c_bpartner_location_id=c_order.c_bpartner_location_id
join c_paymentterm on c_paymentterm.c_paymentterm_id=c_order.c_paymentterm_id
left join c_project on c_project.c_project_id=c_order.c_project_id
where c_order.issotrx='N' and em_dtpo_orderscenario='TYPE1' and c_order.c_order_id=$P{C_Order_ID}
 limit 1]]>
	</queryString>
