	<subDataset name="New Dataset 1">
		<parameter name="DOCUMENT_ID" class="java.lang.String"/>
		<queryString>
			<![CDATA[SELECT C_INVOICE.C_INVOICE_ID,C_ORDER.DOCUMENTNO as CDOC,
round(c_invoiceline.linenetamt) as linenetamt,c_invoiceline.qtyinvoiced,c_invoiceline.priceactual,
C_ORDER.dateordered,C_PROJECT.VALUE,round(C_INVOICE.totallines) as totallines,
 M_PRODUCT.NAME as productname,c_uom.name

FROM C_INVOICE LEFT JOIN C_BPARTNER ON C_INVOICE.C_BPARTNER_ID = C_BPARTNER.C_BPARTNER_ID
JOIN c_invoiceline on c_invoiceline.c_invoice_id=c_invoice.c_invoice_id
join c_uom on c_invoiceline.c_uom_id=c_uom.c_uom_id
JOIN M_PRODUCT on m_product.m_product_id=c_invoiceline.m_product_id
	LEFT JOIN C_ORDER ON C_ORDER.C_ORDER_ID=C_INVOICE.C_ORDER_ID
 JOIN C_PROJECT ON C_PROJECT.C_PROJECT_ID=C_INVOICE.C_PROJECT_ID
	 LEFT JOIN C_BPARTNER_LOCATION ON C_INVOICE.C_BPARTNER_LOCATION_ID = C_BPARTNER_LOCATION.C_BPARTNER_LOCATION_ID
	 WHERE  C_INVOICE.c_INVOICE_ID =$P{DOCUMENT_ID} and c_invoice.issotrx='Y'
                                                            Group by c_invoice.documentno,C_INVOICE.C_INVOICE_ID,C_ORDER.DOCUMENTNO
                              ,C_ORDER.dateordered,C_PROJECT.VALUE,C_INVOICE.totallines,
 c_invoiceline.linenetamt,c_invoiceline.qtyinvoiced,m_product.name,c_invoiceline.priceactual,c_uom.name]]>
		</queryString>
		<field name="c_invoice_id" class="java.lang.String"/>
		<field name="cdoc" class="java.lang.String"/>
		<field name="linenetamt" class="java.math.BigDecimal"/>
		<field name="qtyinvoiced" class="java.math.BigDecimal"/>
		<field name="priceactual" class="java.math.BigDecimal"/>
		<field name="dateordered" class="java.sql.Timestamp"/>
		<field name="value" class="java.lang.String"/>
		<field name="totallines" class="java.math.BigDecimal"/>
		<field name="productname" class="java.lang.String"/>
		<field name="name" class="java.lang.String"/>
	</subDataset>
	<parameter name="DOCUMENT_ID" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT distinct C_INVOICE.C_INVOICE_ID,C_ORDER.DOCUMENTNO as CDOC,C_INVOICE.docstatus,em_dt_taxname,round(sum(c_invoiceline.linenetamt)) as totallines,round(c_invoice.grandtotal) as grandtotal,em_dt_irnno,em_dt_policyno,c_invoice.description as enclosures,c_order.em_dt_vendorcode,em_dt_excisechalan,
C_ORDER.dateordered  as PODATE,c_project.value,
 C_BPARTNER.NAME, C_BPARTNER.TAXID AS CIF, AD_USER.NAME AS CONTACT_NAME,C_LOCATION.ADDRESS1,
C_LOCATION.POSTAL,coalesce(C_LOCATION.CITY,' ') as city,coalesce(C_REGION.NAME,' ') AS REGION,
C_COUNTRY.NAME AS COUNTRY,C_BPARTNER_LOCATION.PHONE AS PHONE,
trim(substring(cash_words((c_invoice.grandtotal:: character varying):: money), 1, position('dollars' in cash_words((c_invoice.grandtotal:: character varying):: money))-1)),c_tax.name as tax,
C_BPARTNER_LOCATION.FAX AS FAX,
C_INVOICE.DATEINVOICED as INVOICEDATE, C_INVOICE.DOCUMENTNO as IDOC ,
 C_BPARTNER.VALUE AS CODE_BPARTNER

FROM C_INVOICE LEFT JOIN C_BPARTNER ON C_INVOICE.C_BPARTNER_ID = C_BPARTNER.C_BPARTNER_ID
join c_invoiceline on c_invoiceline.c_invoice_id=c_invoice.c_invoice_id
join c_tax on c_invoiceline.c_tax_id=c_tax.c_tax_id



      LEFT   JOIN C_ORDER ON C_ORDER.C_ORDER_ID=C_INVOICE.C_ORDER_ID
  LEFT JOIN AD_USER ON C_INVOICE.AD_USER_ID = AD_USER.AD_USER_ID
 JOIN C_PROJECT ON C_PROJECT.C_PROJECT_ID=C_INVOICE.C_PROJECT_ID
         LEFT JOIN C_BPARTNER_LOCATION ON C_INVOICE.C_BPARTNER_LOCATION_ID = C_BPARTNER_LOCATION.C_BPARTNER_LOCATION_ID
                 LEFT JOIN C_LOCATION ON C_BPARTNER_LOCATION.C_LOCATION_ID = C_LOCATION.C_LOCATION_ID
                         LEFT JOIN C_REGION ON C_LOCATION.C_REGION_ID = C_REGION.C_REGION_ID
                                 LEFT JOIN C_COUNTRY ON C_LOCATION.C_COUNTRY_ID = C_COUNTRY.C_COUNTRY_ID
         LEFT JOIN C_CURRENCY ON C_INVOICE.C_CURRENCY_ID = C_CURRENCY.C_CURRENCY_ID
         WHERE  C_INVOICE.c_INVOICE_ID =$P{DOCUMENT_ID} and c_invoice.issotrx='Y'
and c_invoiceline.c_tax_id <> '3692396330B592800130FE7A756D070B'
        Group by C_INVOICE.C_INVOICE_ID,C_ORDER.DOCUMENTNO,em_dt_taxname,c_invoice.totallines,c_invoice.grandtotal ,em_dt_irnno,em_dt_policyno,c_invoice.description
,c_order.em_dt_vendorcode,em_dt_excisechalan,
C_ORDER.dateordered,c_project.value,c_invoiceline.linenetamt,C_BPARTNER.NAME, C_BPARTNER.TAXID, AD_USER.NAME,C_LOCATION.ADDRESS1,
C_LOCATION.POSTAL,C_LOCATION.CITY,C_REGION.NAME,C_COUNTRY.NAME,C_BPARTNER_LOCATION.PHONE,C_BPARTNER_LOCATION.FAX,C_INVOICE.docstatus,
C_INVOICE.DATEINVOICED, C_INVOICE.DOCUMENTNO, C_BPARTNER.VALUE,c_tax.name]]>
	</queryString>
