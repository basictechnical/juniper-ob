left join c_project on c_order.c_project_id=c_project.c_project_id
left join ad_org on c_order.ad_org_id=ad_org.ad_org_id
left join c_bpartner on c_order.c_bpartner_id=c_bpartner.c_bpartner_id
left join c_bpartner_location on c_order.c_bpartner_location_id=c_bpartner_location.c_bpartner_location_id

  where c_order.issotrx='N' and em_dtpo_orderscenario='TYPE4' and c_order.c_order_id=$P{C_Order_ID} limit 1]]>
