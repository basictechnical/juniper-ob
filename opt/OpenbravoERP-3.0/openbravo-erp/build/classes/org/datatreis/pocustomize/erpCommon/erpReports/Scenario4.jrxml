	<subDataset name="New Dataset 1">
		<parameter name="C_Order_ID" class="java.lang.String"/>
		<queryString>
			<![CDATA[select c_order.c_order_id,m_product.value item_range,c_orderline.description dscptn,m_attributevalue.em_dtpo_make make,
m_attributevalue.em_dtpo_model model_no,m_attributevalue.em_dtpo_itemno,
m_attributevalue.em_dtpo_hsncode,m_attributevalue.em_dtpo_countryoforigin,m_attributevalue.em_dtpo_portofloading,c_uom.name as uom,
c_orderline.qtyordered,c_orderline.linenetamt amount,c_orderline.em_dtpo_servicetax,c_orderline.em_dtpo_installationtotal,c_orderline.em_dtpo_others ::numeric,em_dtpo_total total,c_orderline.em_dtpo_cess cess,c_orderline.em_dtpo_vat_cst as vat
from c_orderline

left join c_order on c_orderline.c_order_id=c_order.c_order_id
left join m_product on c_orderline.m_product_id=m_product.m_product_id
left join m_attributesetinstance on m_attributesetinstance.m_attributesetinstance_id=c_orderline.m_attributesetinstance_id
left join m_attributeset on m_attributeset.m_attributeset_id=m_attributesetinstance.m_attributeset_id
left join m_attributeuse on m_attributeuse.m_attributeset_id=m_attributeset.m_attributeset_id
left join m_attributevalue on m_attributevalue.m_attribute_id=m_attributeuse.m_attribute_id

left join c_uom on c_orderline.c_uom_id=c_uom.c_uom_id
where c_order.issotrx='N' and em_dtpo_orderscenario='TYPE4' and c_order.c_order_id=$P{C_Order_ID}]]>
		</queryString>
		<field name="c_order_id" class="java.lang.String"/>
		<field name="item_range" class="java.lang.String"/>
		<field name="dscptn" class="java.lang.String"/>
		<field name="make" class="java.lang.String"/>
		<field name="model_no" class="java.lang.String"/>
		<field name="em_dtpo_itemno" class="java.lang.String"/>
		<field name="em_dtpo_hsncode" class="java.lang.String"/>
		<field name="em_dtpo_countryoforigin" class="java.lang.String"/>
		<field name="em_dtpo_portofloading" class="java.lang.String"/>
		<field name="uom" class="java.lang.String"/>
		<field name="qtyordered" class="java.math.BigDecimal"/>
		<field name="amount" class="java.math.BigDecimal"/>
		<field name="em_dtpo_servicetax" class="java.math.BigDecimal"/>
		<field name="em_dtpo_installationtotal" class="java.math.BigDecimal"/>
		<field name="em_dtpo_others" class="java.math.BigDecimal"/>
		<field name="total" class="java.math.BigDecimal"/>
		<field name="cess" class="java.math.BigDecimal"/>
		<field name="vat" class="java.math.BigDecimal"/>
		<variable name="AUTO_COUNT" class="java.lang.Integer">
			<variableExpression><![CDATA[new Integer($V{AUTO_COUNT}.intValue() + 1)]]></variableExpression>
			<initialValueExpression><![CDATA[new Integer(0)]]></initialValueExpression>
		</variable>
		<variable name="total_sum" class="java.math.BigDecimal" calculation="Sum">
			<variableExpression><![CDATA[$F{total}]]></variableExpression>
		</variable>
		<variable name="lab_cess" class="java.math.BigDecimal">
			<variableExpression><![CDATA[$F{cess}.multiply( $F{qtyordered} )]]></variableExpression>
		</variable>
		<variable name="serv_qty" class="java.math.BigDecimal">
			<variableExpression><![CDATA[$F{em_dtpo_servicetax}.multiply($F{qtyordered} )]]></variableExpression>
		</variable>
		<variable name="serv_tax_total" class="java.math.BigDecimal" calculation="Sum">
			<variableExpression><![CDATA[$V{serv_qty}]]></variableExpression>
		</variable>
		<variable name="lab_cess_total" class="java.math.BigDecimal" calculation="Sum">
			<variableExpression><![CDATA[$V{lab_cess}]]></variableExpression>
		</variable>
		<variable name="other_qty" class="java.math.BigDecimal">
			<variableExpression><![CDATA[$F{em_dtpo_others}.multiply($F{qtyordered})]]></variableExpression>
		</variable>
		<variable name="other_total" class="java.math.BigDecimal" calculation="Sum">
			<variableExpression><![CDATA[$V{other_qty}]]></variableExpression>
		</variable>
		<variable name="Net_vat" class="java.math.BigDecimal">
			<variableExpression><![CDATA[$F{qtyordered}.multiply( $F{vat} )]]></variableExpression>
		</variable>
		<variable name="Tot_vat" class="java.math.BigDecimal" calculation="Sum">
			<variableExpression><![CDATA[$V{Net_vat}]]></variableExpression>
		</variable>
		<variable name="grand_total" class="java.math.BigDecimal">
			<variableExpression><![CDATA[$V{total_sum}.add( $V{serv_tax_total} ).add($V{lab_cess_total}).add($V{other_total}).add($V{Tot_vat})]]></variableExpression>
		</variable>
	</subDataset>
