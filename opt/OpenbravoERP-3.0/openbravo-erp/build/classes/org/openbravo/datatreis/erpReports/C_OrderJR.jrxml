	<subDataset name="subreportdataset" whenResourceMissingType="Empty">
		<parameter name="DOCUMENT_ID_SUB" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT	COALESCE(m_product."name"||'('||c_orderline."description"||')',m_product."name"||'('||replace(m_attributesetinstance.description,'_','*')||')',m_product."name", c_orderline."description") AS Product_desc,
		c_orderline."qtyordered" AS c_orderline_qtyordered,
		c_orderline."priceactual" AS c_orderline_priceactual,
		obuiapp_note.note,
		c_orderline."linenetamt" AS c_orderline_linenetamt,
		((round(c_order."grandtotal",0)||'.00')::numeric) AS c_order_grandtotal,
		c_order."totallines" AS totallines,
		COALESCE(alt.NAME,C_UOM.NAME) AS c_uom_name,
		(CASE WHEN to_number(c_project.value) BETWEEN 2000 AND 9000 THEN 'For HARSHINI EPC PRIVATE LIMITED'||E'\r\n\n' || 'Chief Operating Officer' ELSE 'For HARSHINI EPC PRIVATE LIMITED '||E'\r\n\n' || ' Chief Operating Officer' END) AS prjcthead,
		upper(c_doctype.name) as headerLabel,
		array_to_string(ARRAY(select row_number() over (order by c_file.name) || '. '||c_file.name from c_file
		where c_file.ad_record_id= '' group by c_file.name),E'\r\n') as encolsures
FROM 		c_orderline
LEFT JOIN  	m_attributesetinstance on m_attributesetinstance.m_attributesetinstance_id =c_orderline.m_attributesetinstance_id
LEFT JOIN  	m_product on m_product.m_product_id=c_orderline.m_product_id
INNER JOIN	c_order ON c_order.c_order_id=c_orderline.c_order_id
left join       obuiapp_note on obuiapp_note.record_id=c_order.c_order_id and obuiapp_note.ad_table_id='259'
LEFT JOIN 	c_project ON c_project.c_project_id=c_order.c_project_id
INNER JOIN	c_uom ON c_uom.c_uom_id=m_product.c_uom_id
LEFT JOIN 	M_PRODUCT_UOM ON c_orderline.M_PRODUCT_UOM_ID = M_PRODUCT_UOM.M_PRODUCT_UOM_ID
LEFT JOIN 	C_UOM alt ON M_PRODUCT_UOM.C_UOM_ID = alt.C_UOM_ID,

c_doctype
WHERE 		c_order.c_order_id = $P{DOCUMENT_ID_SUB}

AND 		c_order.c_doctypetarget_id=c_doctype.c_doctype_id
GROUP BY 	m_product.name,c_orderline_qtyordered,
		c_orderline_priceactual,c_orderline_linenetamt,
		c_order_grandtotal,totallines,c_uom_name,obuiapp_note.note,
		c_project.value,c_doctype.name,c_orderline.line,Product_desc
ORDER BY 	c_orderline.line]]>
		</queryString>
		<field name="product_desc" class="java.lang.String"/>
		<field name="c_orderline_qtyordered" class="java.math.BigDecimal"/>
		<field name="c_orderline_priceactual" class="java.math.BigDecimal"/>
		<field name="note" class="java.lang.String"/>
		<field name="c_orderline_linenetamt" class="java.math.BigDecimal"/>
		<field name="c_order_grandtotal" class="java.math.BigDecimal"/>
		<field name="totallines" class="java.math.BigDecimal"/>
		<field name="c_uom_name" class="java.lang.String"/>
		<field name="prjcthead" class="java.lang.String"/>
		<field name="headerlabel" class="java.lang.String"/>
		<field name="encolsures" class="java.lang.String"/>
	</subDataset>
