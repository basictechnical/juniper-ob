	<queryString>
		<![CDATA[select
     c_order."documentno" AS documentno,
     c_order."deliverynotes" AS poref,
     to_char(date_trunc('day',min(c_order."dateordered"))) AS c_order_dateordered,
    to_char(date_trunc('day',min(c_order."datepromised"))) AS deliverynotes,
     (c_order."documentno"||' '||'Dt. '||to_char(date_trunc('day',min(c_order."dateordered")))||')') AS powithdate,
     round(c_order."grandtotal",0) AS c_order_grandtotal,
     c_order."totallines" AS totallines,
     c_order."description" AS description_po,
     c_location."address1" AS c_location_address1,
     c_location."address2" AS c_location_address2,
     c_location."city" AS c_location_city,
     c_location."postal" AS c_location_postal,
     c_country."name" AS c_location_c_country_id,
     c_bpartner."taxid" AS c_bpartner_taxid,
     c_bpartner."name" AS c_bpartner_name,
     ad_user."name" AS ad_user_name,
     ad_user."email" AS ad_user_email,
     c_bpartner_location."phone" AS ad_user_phone,
     c_bpartner_location."phone2" AS ad_user_phone2,
     ad_user."fax" AS ad_user_fax,
     ad_user."firstname" AS ad_user_firstname,
     fin_paymentmethod."name" AS payment_method,
     m_pricelist."name" AS price_list,
     Coalesce(c_order."deliverynotes",'Before ' || to_char(date_trunc('day',min(c_order.datepromised)))) AS scheduled_date,
     array_to_string(array(select obuiapp_note.note
    FROM obuiapp_note join ad_table on
  ad_table.ad_table_id=obuiapp_note.ad_table_id and
  ad_table.tablename ='C_Order'
  and record_id =$P{DOCUMENT_ID}  group by note), E'\n') AS additionalFields,
  dt_order_tc.description AS freight,
  dt_order_tc.price_basis AS pricebasis,
  dt_order_tc.pkgng_frwd AS pkgngfrwd,
 Coalesce(dt_order_tc.exice,'NOT APPLICABLE') AS exice,
Coalesce(dt_order_tc.sales_tax,'NOT APPLICABLE') AS sales_tax,
Coalesce(dt_order_tc.transit_ins,'NOT APPLICABLE') AS transit_ins,
Coalesce(dt_order_tc.delivery,'NOT APPLICABLE') AS delivery,
Coalesce(dt_order_tc.inspection,'NOT APPLICABLE') AS inspection,
Coalesce(dt_order_tc.quality,'NOT APPLICABLE') AS quality,
Coalesce(dt_order_tc.disposal,'NOT APPLICABLE') AS disposal,
Coalesce(dt_order_tc.dispatch,'NOT APPLICABLE') AS dispatch,
Coalesce(dt_order_tc.pay_terms,'NOT APPLICABLE') AS pay_terms,
Coalesce(dt_order_tc.pay_mthd,'NOT APPLICABLE') AS pay_mthd,
Coalesce(dt_order_tc.warranty,'NOT APPLICABLE') AS warranty,
Coalesce(dt_order_tc.assignmnt,'NOT APPLICABLE') AS assignmnt,
Coalesce(dt_order_tc.rejction || ' ' || c_bpartner."name",'NOT APPLICABlE') AS rejction,
Coalesce(dt_order_tc.liquid_dmg,'NOT APPLICABLE') AS liquid_dmg,
Coalesce(dt_order_tc.order_cncl,'NOT APPLICABLE') AS order_cncl,
Coalesce(dt_order_tc.incme_tax,'NOT APPLICABLE') AS incme_tax,
Coalesce(dt_order_tc.reg_act,'NOT APPLICABLE') AS reg_act,
Coalesce(dt_order_tc.corspndc,'NOT APPLICABLE') AS corspndc,
Coalesce(dt_order_tc.arbt,'NOT APPLICABLE') AS arbt,
Coalesce(dt_order_tc.consign_detail,'NOT APPLICABLE') AS consign_detail,
Coalesce(dt_order_tc.tax_invoice,'NOT APPLICABLE') AS tax_invoice,
dt_order_tc.add_info AS additional_info,
CASE WHEN to_number(c_project.value) BETWEEN 2000 AND 9000 THEN true ELSE false END,
upper(c_doctype.name) as headerLabel,
(CASE WHEN c_order.issotrx='Y' THEN 'CUSTOMER' ELSE 'VENDOR' END) as bptype,
upper(c_doctype.name||' No.') as bptype_number_label,
(CASE WHEN (to_number(c_project.value) BETWEEN 2000 AND 9000)  THEN  ' HARSHINI EPC PRIVATE LIMITED'
when c_project.value LIKE 'CPO%' THEN  ' HARSHINI EPC PRIVATE LIMITED'
ELSE 'HARSHINI EPC PRIVATE LIMITED' END) AS prjctcompny,
CASE WHEN to_number(c_project.value)  BETWEEN 2000 AND 9000 THEN '28849380131'
WHEN trim(c_project.value,'CPO-') BETWEEN  '12130000' and '12138000'  THEN '28849380131'
WHEN c_order.dateordered >= (date '2012-04-01') AND to_number(c_project.value) BETWEEN 2000 AND 9000 THEN '28849380131' ELSE '28970185432'  END AS vattinno,
(CASE WHEN dt_order_tc.isactive='N' THEN false ELSE true END) AS terms_conditions
FROM c_order join c_project on c_project.c_project_id=c_order.c_project_id
left join c_bpartner_location on c_bpartner_location.c_bpartner_location_id=c_order.c_bpartner_location_id
left join ad_user on ad_user.c_bpartner_location_id= c_bpartner_location.c_bpartner_location_id
left join dt_order_tc on dt_order_tc.c_order_id=c_order.c_order_id
left join c_doctype on c_doctype.c_doctype_id=c_order.c_doctypetarget_id,
c_bpartner left join m_product on m_product.c_bpartner_id=c_bpartner.c_bpartner_id,
m_pricelist,fin_paymentmethod,
  c_location left join c_country on c_country.c_country_id=c_location.c_country_id
  where c_order.c_order_id =$P{DOCUMENT_ID}
  and c_order.c_bpartner_id=c_bpartner.c_bpartner_id
  and c_bpartner_location.c_location_id=c_location.c_location_id
  and m_pricelist.m_pricelist_id=c_order.m_pricelist_id
  and fin_paymentmethod.fin_paymentmethod_id=c_order.fin_paymentmethod_id
  group by
  documentno,poref,deliverynotes,
  c_order_grandtotal,description_po,c_bpartner_taxid,
  c_bpartner_name,price_list,totallines,payment_method,
  c_location_address1,c_location_address2,c_location_city,
  c_location_postal,ad_user_name,ad_user_email,ad_user_phone,
  ad_user_phone2,ad_user_fax,ad_user_firstname,c_location_c_country_id,freight,
  pricebasis,pkgngfrwd,exice, sales_tax, transit_ins, delivery, inspection,
       quality, disposal, dispatch, pay_terms, pay_mthd, warranty, assignmnt,
       rejction, liquid_dmg, order_cncl, incme_tax, reg_act, corspndc,
       arbt, consign_detail,dateordered, tax_invoice,c_project.value,c_order.issotrx,c_doctype.name,dt_order_tc.add_info,dt_order_tc.isactive;]]>
	</queryString>
